use std

#typ size_t
#typ FILE
#register @FILE fopen(@byte, @byte)
#register int fclose(@FILE)
#register int fread(@FILE)

fn main([][]byte args) {
    if (args.len[0] < 2) {
        printf("Usage %s <file.fd>\n", args[0])
        ret 1
    }
    printf("Opening %s\n", args[1].data)
    file = fopen(args[1].data, "r")
    fclose(file)
    ret 0
}